{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "implicit-arrow-linebreak": 0,
        "comma-dangle": ["error", "never"],
        "no-unused-vars": ["off"],
        "import/prefer-default-export": 0,
        "indent": [2, 4],
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "no-constant-condition": 0
    }
}
